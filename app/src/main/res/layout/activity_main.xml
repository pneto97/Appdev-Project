<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/purple">


    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="83dp"
        android:layout_height="96dp"
        android:layout_above="@+id/imagemExibicaoCorvali"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="-47dp"
        app:srcCompat="@drawable/igor_barra" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="27dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="13dp"
        android:layout_marginTop="7dp"
        app:srcCompat="@drawable/botao_menu_navegacao" />

    <ImageView
        android:id="@+id/menuDireitoid"
        android:layout_width="7dp"
        android:layout_height="34dp"
        android:layout_alignEnd="@+id/button_new_adventure"
        android:layout_alignParentTop="true"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="1dp"
        app:srcCompat="@drawable/botao_dropdown" />

    <ImageView
        android:id="@+id/imagemExibicaoCorvali"
        android:layout_width="match_parent"
        android:layout_height="98dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="48dp"
        android:background="@drawable/corvali_overlay" />

    <TextView
        android:id="@+id/nomeAventuraCorvaliid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="52dp"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textSize="20sp" />


    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="match_parent"
        android:layout_height="99dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="142dp"
        android:background="@drawable/automatica_overlay" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="match_parent"
        android:layout_height="109dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="217dp"
        android:background="@drawable/coast_overlay" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="match_parent"
        android:layout_height="113dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="104dp"
        android:background="@drawable/heartlands_overlay" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="match_parent"
        android:layout_height="104dp"
        android:layout_alignBottom="@+id/button_new_adventure"
        android:layout_alignParentStart="true"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="-46dp"
        android:background="@drawable/krevast_overlay" />

    <ImageButton
        android:id="@+id/button_new_adventure"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="46dp"
        android:background="@drawable/botao_criar_nova_aventura" />

    <TextView
        android:id="@+id/textProgressCorvaliid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/nomeAventuraCorvaliid"
        android:layout_alignTop="@+id/imagemExibicaoCorvali"
        android:layout_marginTop="49dp"
        android:textColor="@color/common_google_signin_btn_text_dark_default" />

    <SeekBar
        android:id="@+id/seekBarCorvaliid"
        android:layout_width="325dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imagemExibicaoCorvali"
        android:layout_marginStart="-22dp"
        android:layout_marginTop="62dp"
        android:layout_toEndOf="@+id/imageView3" />


</RelativeLayout>
